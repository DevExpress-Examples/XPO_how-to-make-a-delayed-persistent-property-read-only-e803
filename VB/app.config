<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="4">
			<listeners>
				<add name="LogFileTraceListener" type="System.Diagnostics.ConsoleTraceListener" />
			</listeners>
		</trace>
		<switches>
			<add name="XPO" value="3" />
		</switches>
	</system.diagnostics>
</configuration>